import wollok.game.*
import snake.*
import configuracion.*

program snake {
	//  CONFIG	
	game.title("Snake")
	game.height(9)
	game.width(9)
	game.ground("ground.png")
	config.configurarTeclas()
	game.onTick(500, "Movimiento del Snake", {snake.actualizarMovimiento()})

	//  VISUALES
 	game.addVisual(snake)
 	game.showAttributes(snake)

 	//  COMIENZA
	game.start()
}